<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- maven build 시 mapper xml찾지못하는 오류 방지하기위해 임시로 작성한 mapper xml -->
<mapper namespace="kr.ync.project.mapper.UserMapper">


	<select id="listAll" resultType="NoticeBoardVO">
		select id, passwd, name from ta_member where id = #{ID} and #{PASSWD}
	</select>
	
</mapper>
