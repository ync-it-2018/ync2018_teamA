<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- maven build 시 mapper xml찾지못하는 오류 방지하기위해 임시로 작성한 mapper xml -->
<mapper namespace="kr.ync.project.mapper.TipNoticeBoardMapper">

<select id="listAll" resultType="TipNoticeBoardVO">
<![CDATA[
SELECT
BOARD_IDX, TITLE, WRITEDATE
FROM
TA_BOARD
WHERE BOARD_IDX > 0 AND BOARD_CODE = '01'
]]>
</select>
</mapper>
